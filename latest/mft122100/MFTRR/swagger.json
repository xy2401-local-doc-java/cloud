                },
                "errorKey":{
                    "type":"string",
                    "description":"The error key that Oracle Managed File Transfer returns."
                },
                "errorMessage":{
                    "type":"string",
                    "description":"A message describing the error that Oracle Managed File Transfer returns."
                }
            }
        },
