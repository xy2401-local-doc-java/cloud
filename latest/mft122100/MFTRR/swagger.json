                                "bindingType":{
                                    "description":"Binding type of the source.",
                                    "type":"string"
                                },
                                "eventInvokeUrl":{
                                    "description":"URL at which to invoke the event.",
                                    "type":"string"
                                }
                            }
                        }
                    ]
                }
            }
