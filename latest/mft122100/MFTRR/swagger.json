                },
                "totalInstances":{
                    "type":"integer",
                    "description":"Total number of instances created out of this event."
                },
                "sourceName":{
                    "type":"string",
                    "description":"Name of the source."
                },
                "sourceType":{
                    "type":"string",
                    "description":"Depicts the source types for which the event was executed (File, FTP, SFTP, etc.)."
                },
                "endpointReference":{
